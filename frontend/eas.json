{
  "cli": {
    "version": ">= 11.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "prebuildCommand": "npx expo prebuild --clean"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "prebuildCommand": "npx expo prebuild --clean",
      "ios": {
        "simulator": false,
        "enterpriseProvisioning": "adhoc"
      }
    },
    "production": {
      "channel": "production",
      "prebuildCommand": "npx expo prebuild --clean"
    }
  },
  "submit": {
    "production": {}
  }
}

