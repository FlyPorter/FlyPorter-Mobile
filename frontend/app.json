{
  "expo": {
    "name": "FlyPorter",
    "slug": "flyporter",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flyporter.mobile",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.flyporter.mobile",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ]
    },

    "web": {
      "favicon": "./assets/icon.png"
    },

    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "FlyPorter"
    },

    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#000000",
          "sounds": []
        },
        "expo-splash-screen"
      ]
    ],

    "extra": {
      "eas": {
        "projectId": "d24d8c01-c9a6-4c03-ba07-0c858aa49836"
      }
    },

    "updates": {
      "url": "https://u.expo.dev/d24d8c01-c9a6-4c03-ba07-0c858aa49836"
    },

    "runtimeVersion": "1.0.1"
  }
}
